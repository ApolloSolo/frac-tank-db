<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>All Volumes</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="">
    </head>
    <body>
        <h1>All Volumes Page</h1>
        <a href="/volumes/new"><button>Add Volume</button></a>

        <ul>
            
            <%for(let v of volumes){%>
                <%if(v.tank[1] === "A"){%>
                    <li><%=v.tank%></li>
                <ul>
                    <li><b>Total:</b> <%=v.totalV%> <b>Water:</b> <%=v.waterV%> <b>Product:</b> <%=v.productV%></li>
                </ul>
                <%}%>
            <%}%>

        </ul>

        <ul>
            
            <%for(let v of volumes){%>
                <%if(v.tank[1] === "B"){%>
                    <li><%=v.tank%></li>
                <ul>
                    <li><b>Total:</b> <%=v.totalV%> <b>Water:</b> <%=v.waterV%> <b>Product:</b> <%=v.productV%></li>
                </ul>
                <%}%>
            <%}%>

        </ul>
        
        <a href="/">Home</a>
        
        <script src="" async defer></script>
    </body>
</html>